# Database Configuration
# For local development:
DATABASE_URL=postgresql://postgres:postgres@localhost:5432/agentpipe
# For Docker Compose (automatically set in docker-compose.yaml):
# DATABASE_URL=postgresql://postgres:postgres@db:5432/agentpipe

# AgentPipe Configuration
AGENTPIPE_BRIDGE_API_KEY=your_bridge_api_key

# Application Configuration
NODE_ENV=development
NEXT_PUBLIC_API_URL=http://localhost:3000

# Supabase Configuration (Optional)
# Only needed if using Supabase for authentication or storage
NEXT_PUBLIC_SUPABASE_URL=your_supabase_project_url
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key
